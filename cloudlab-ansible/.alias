# Get the ip-addr of a docker container
dip() {
    docker inspect -f '{{range.NetworkSettings.Networks}}{{.IPAddress}}{{end}}' $1
}

# Find the process info
fps(){
    ps aux | grep "$1"
}
alias lx='ll -haXtr'
alias xinda='cd ~/workdir/xinda'
alias xindata='cd ~/workdir/data'
alias al='cat ~/.alias' # Check the current list of alias
alias dps='docker ps -a'