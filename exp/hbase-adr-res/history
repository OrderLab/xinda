for (cpu in c(1,2,5)) {
  for (mem in c(8,16,32)) {
    cat(paste0("cd ~/workdir/hbase-pipeline/figure/workload-change; python3 different-resource.py --cpu ",cpu," --mem ", mem, "\n"))
  }
}


cd ~/workdir/hbase-pipeline/figure/workload-change; python3 different-resource.py --cpu 1 --mem 8
