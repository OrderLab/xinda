ansible-playbook -i ../exp/hbase-adr-diff-wkl/hosts compress_exp_data_hbase_finetune.yml --extra-vars "task=hbase-adr-diff-wkl"
for (tc in c(1,8,32)) {
  for (rc in c('10000','1000000')) {
    for (wkl in c('readonly','writeonly','mixed')) {
      cat(paste0("cd ~/workdir/hbase-pipeline/figure/workload-change; python3 different-workload.py --rc ",rc," --tc ", tc," --wkl ",wkl, "\n"))
    }
  }
}


cd ~/workdir/hbase-pipeline/figure/workload-change; python3 different-workload.py --rc 10000 --tc 1 --wkl readonly
